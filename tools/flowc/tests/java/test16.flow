import runtime;

// Optimization of const expr boolean negation

main() {
	println(!true);
	println(!false);
	println(!!true);
	println(!!false);
	println(!!!true);
	println(!!!false);
	
	println(!(1 == 1));
	println(!(2 != 2));
	println(!!(1 == 1));
	println(!!(2 != 2));
	println(!!!!!(1 == 1));
	println(!!!!!(2 != 2));
}
