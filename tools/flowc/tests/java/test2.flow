import runtime;

// Optimization of const expr addition 

main() {
	println(1 + 2);
	println(3.1 + 4.1);
	println("abc" + "def");
	println(1 + 2 + 3);
	println(3.1 + 4.1 + 5.2);
	println("abc" + "def" + "ghi");
	println((1 + 2) + (3 + 4));
	println((3.1 + 4.1) + (5.2 + 4.5));
	println(("abc" + "def") + ("ghi" + "jkl"));
}
