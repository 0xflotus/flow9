import runtime;

// Optimization of const expr boolean or

main() {
	println(true || true);
	println(true || false);
	println(false || true);
	println(false || false);
	
	println((1 == 1) || (2 == 2));
	println((1 == 1) || (2 != 2));
	println((1 != 1) || (2 == 2));
	println((1 != 1) || (5 != 5));
}
