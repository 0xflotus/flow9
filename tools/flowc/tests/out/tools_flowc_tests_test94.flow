WigiRecursiveType ::= WigiComap, WigiFraction, WigiProduct, WigiEvaluatingIntegral, WigiHyperlink;
				WigiComap();
				WigiFraction();
				WigiProduct();
				WigiEvaluatingIntegral();
				WigiRoot();
				WigiHyperlink();

getDefaultElementStyle(type) -> string {
	switch (type : WigiRecursiveType) {
		WigiFraction(): type.structname;
		WigiComap(): type.structname;
		WigiHyperlink(): type.structname;
		default: "";
	}
}

----------

getDefaultElementStyle = (\type:WigiRecursiveType ->
	(switch (type : WigiRecursiveType) {
		WigiFraction(): {
			((type : WigiFraction).structname : string)
		}
		WigiComap(): {
			((type : WigiComap).structname : string)
		}
		WigiHyperlink(): {
			((type : WigiHyperlink).structname : string)
		}
		default(): {
			("" : string)
		}
	} : string)
 : (type : WigiRecursiveType) -> string);
