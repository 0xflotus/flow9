native fold : (xs : [?], init : ??, fn : (??, ?)->??) -> ?? = Native.fold;
native concat : ([?], [?]) -> [?] = Native.concat;

strWordWrap() -> [string] {
	// debugtyping;
	fold([], [""], \r, t -> {
		if (true) concat([r[0]+t], r) 
		else concat([t], r);
	});
}

