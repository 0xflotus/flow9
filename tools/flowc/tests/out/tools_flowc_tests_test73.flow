strWordWrap = (\ ->
	((fold : (xs : [string], init : [string], fn : ([string], string) -> [string]) -> [string])(([] : [string]), ([("" : string)] : [string]), (\r:[string], t:string ->
		(if ((true : bool)) {
			((concat : ([string], [string]) -> [string])(([((((r : [string])[(0 : int)] : string) + (t : string)) : string)] : [string]), (r : [string])) : [string])
		} else {
			((concat : ([string], [string]) -> [string])(([(t : string)] : [string]), (r : [string])) : [string])
		}
		 : [string])
	 : (r : [string], t : string) -> [string])) : [string])
 : () -> [string]);
