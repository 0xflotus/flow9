Transform<?> ::= FSelect2<?>;

FSelect2(b1 : Transform<flow>, b2 : Transform<flow>, mutable beh : FBehaviour<?>);

FBehaviour(value : ?);

transformLength(s : Transform<?>) -> int {
	switch (s : Transform<?>) {
		FSelect2(b1, b2, __): {
			transformLength(b1) + transformLength(b2);
		}
	}
}

