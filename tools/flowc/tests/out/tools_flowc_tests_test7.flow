// Apply a collecting function.  This is a left fold, i.e., it folds with the start of
// the array first, i.e., fold([x1, x2, x3], x0, o) = ((x0 o x1) o x2) o x3
native fold : (xs : [?], init : ??, fn : (??, ?)->??) -> ?? = Native.fold;

// a = debugtyping1;

unzipB(a: [Pair<?, ??>]) -> bool {
	fold(a, true, \acc, p -> {
		p.first; p.second; acc
	})
}

