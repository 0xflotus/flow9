unzipB = (\a:[Pair<?, ??>] ->
	((fold : (xs : [Pair<?, ??>], init : bool, fn : (bool, Pair<?, ??>) -> bool) -> bool)((a : [Pair<?, ??>]), (true : bool), (\acc:bool, p:Pair<?, ??> ->
		({
			((p : Pair<?, ??>).first : ?);
			((p : Pair<?, ??>).second : ??);
			(acc : bool)
		}
		 : bool)
	 : (acc : bool, p : Pair<?, ??>) -> bool)) : bool)
 : (a : [Pair<?, ??>]) -> bool);
