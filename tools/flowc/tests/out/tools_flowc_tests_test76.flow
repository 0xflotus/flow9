
native isSameStructType : (value1 : flow, value2 : flow) -> bool = Native.isSameStructType;
native concat : ([?], [?]) -> [?] = Native.concat;
native filter : (a : [?], test : (?) -> bool) -> [?] = Native.filter;

// a = debugtyping;
replaceStruct(a : [?], e : ??) -> [?] {
	concat(filter(a, \el : ? -> !isSameStructType(el, e)), [e])
}

