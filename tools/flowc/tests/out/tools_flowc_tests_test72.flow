			(Pair(((acc : Pair<[?], [?]>).first : [?]), ((arrayPush : (array : [?], value : ?) -> [?])(((acc : Pair<[?], [?]>).second : [?]), (x : ?)) : [?])) : Pair<[?], [?]>)
		}
		 : Pair<[?], [?]>)
