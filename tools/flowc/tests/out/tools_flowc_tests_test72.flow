		(if (((fn : (?) -> bool)((x : ?)) : bool)) {
			(Pair(((arrayPush : (array : [?], value : ?) -> [?])(((acc : Pair<[?], [?]>).first : [?]), (x : ?)) : [?]), ((acc : Pair<[?], [?]>).second : [?])) : Pair<[?], [?]>)
