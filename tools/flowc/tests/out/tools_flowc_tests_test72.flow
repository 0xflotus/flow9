Pair(first : ?, second : ??);

native fold : (xs : [?], init : ??, fn : (??, ?)->??) -> ?? = Native.fold;

arrayPush(array : [?], value : ?)->[?] { 
	array
}

unzip(xs : [?], fn : (?) -> bool) -> Pair<[?], [?]> {
//	debugtyping;
	fold(xs, Pair([], []), \acc : Pair<[?], [?]>, x -> {
		if (fn(x)) {
			Pair(
				arrayPush(acc.first, x),
				acc.second
			)
		} else {
			Pair(
				acc.first,
				arrayPush(acc.second, x)
			)
		}
	})
}


