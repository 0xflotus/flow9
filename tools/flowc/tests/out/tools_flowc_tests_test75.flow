Maybe<?> ::= None, Some<?>;
None();
Some(value : ?);

// a = debugtyping;

maybeMap(m, f) {
	switch (m : Maybe) {
		None(): None();
		Some(v): Some(f(v));
	}
}

----------

maybeMap = (\m:Maybe<?>, f:(?) -> ?? ->
	(switch (m : Maybe<?>) {
		None(): {
			(None() : None<??>)
		}
		Some(v): {
			(v : ? = ((m : Some<?>).value : ?);
			(Some(((f : (?) -> ??)((v : ?)) : ??)) : Some<??>) : Some<??>)
		}
	} : Maybe<??>)
 : (m : Maybe<?>, f : (?) -> ??) -> Maybe<??>);
