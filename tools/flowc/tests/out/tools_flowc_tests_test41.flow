GestureState ::= GestureStateBegin, GestureStateProgress, GestureStateEnd;
GestureStateBegin : (); GestureStateProgress : (); GestureStateEnd : ();

intGestureStateToFlowGestureState  = \state -> if (state == 1) GestureStateProgress() else if (state == 0) GestureStateBegin() else GestureStateEnd();

----------

intGestureStateToFlowGestureState = (\state:int ->
	(if ((((state : int) == (1 : int)) : bool)) {
		(GestureStateProgress() : GestureStateProgress)
	} else {
		(if ((((state : int) == (0 : int)) : bool)) {
			(GestureStateBegin() : GestureStateBegin)
		} else {
			(GestureStateEnd() : GestureStateEnd)
		}
		 : GestureState)
	}
	 : GestureState)
 : (state : int) -> GestureState);
