import fusion;
import transforms;

Struct ::= Struct1, Struct2;
	Struct1 : (style : string);
	Struct2 : (style : string);

main() {
	a : DynamicBehaviour<Struct> = make(Struct1("flowc"));
	
	selectu(a, \line -> line.style); // Works fine

	fselect(a, FLift(\line -> line.style)) // Crashes
	|> fuse;

	next(a, Struct2("java"));
}
