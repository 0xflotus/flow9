import sys/interactive;

native prints : (string) -> void = host.prints;
native printd : (double) -> void = host.printd;
native makeClip_a : io () -> native = RenderSupport.makeClip;

main() {
	prints("start");

	clip = makeClip_a();

	x = getMouseX(clip);
	y = getMouseY(clip);

	printd(x);
	printd(y);

	prints("finish");
}
