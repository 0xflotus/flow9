import net/url_parameter;

native prints : (string) -> void = host.prints;

applyParam = {
	t = getUrlParameter("test");
	val = {
		m = if (t != "") 2.0 else 1.0;
		m;
	};

	\str -> {
		str + ": " + d2s(val);
	}
}


main() {
	prints("start");

	s = "call applyParam" |> applyParam;
	prints(s);

	prints("finish");
}
