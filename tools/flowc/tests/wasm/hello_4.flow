import sys/fullscreen;

native prints : (string) -> void = host.prints;

main() {
	prints("start");

	d = onFullScreen(\isFS -> {
		prints("onFullScreen: " + (if(isFS) "true" else "false"));
	});

	// in fact it will not work since user action required
	// toggleFullScreen(true);
	// toggleFullScreen(false);

	prints("finish");
}
