import runtime;

// Optimization of const expr equality

main() {
	println(1 == 2);
	println(1 == 1);
	println(3.1 == 4.1);
	println(3.1 == 3.1);
	println("abc" == "def");
	println("abc" == "abc");
	
	println((1 == 2) == (3.1 == 4.1));
	println((1 == 2) == (3.1 == 3.1));
	println((1 == 1) == (3.1 == 4.1));
	println((1 == 1) == (3.1 == 3.1));
	
	println(true == false);
	println(true == true);
	println(false == false);
	println(false == true);
}

