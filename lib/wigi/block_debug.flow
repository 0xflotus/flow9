// The whole file exists for debugging purposes only, really

import wigi/types;
import date;

export {
	wigiBlockToString(type : WigiBlockType) -> string;
}

wigiBlockToString(type : WigiBlockType) -> string {
	switch (type : WigiBlockType) {
		WigiPicture(url, style): toString(type);
		WigiVideo(url, style): toString(type);
		WigiAudio(url, style): toString(type);
		WigiSlider(val, minVal, maxVal, stepVal, widthVal, style): "Slider " + d2s(getValue(val));
		WigiCheckBox(val, style): "Check box " + b2s(getValue(val));
		WigiRadioButton(val, style): "Radio button" + b2s(getValue(val));
		WigiInputBox(val, w, h, style): "Input box " + getValue(val);
		WigiPositionTag(id): {
			"Position tag " + i2s(id);
		}
		WigiCustomSymbol(t, s): "Custom symbol " + toString(t);
		WigiFormdesigner(drawing, style): "Formdesigner form" + drawing;
		WigiSmartArt(content, style): "SmartArt object " + content;
		WigiFormula(formula, style): "Formula " + formula;
		WigiPageBreak(a, s): "Page break: append=" + b2s(a) + ", autoScroll=" + b2s(s);
		WigiCoach(text): "Coach: " + text;
		WigiPageName(name, disableCondition): "Page name: " + name;
		WigiNaturalMath(formula): "Math formula: " + formula;
		WigiComboBox(s, v, r, w): "Combo box: " + v;
		WigiNativeWiki(w): "Native wigi: " + w;
		WigiDotplot(r, s): "WigiDotplot range: " + r;
		WigiHistogram(r, s): "WigiHistogram range: " + r;
		WigiHistogramClasses(r, s): "WigiHistogramClasses range: " + r;
		WigiBoxplot(r, s): "WigiBoxplot range: " + r;
		WigiScatterplot(r, s): "WigiScatterplot range: " + r;
		WigiStackedBar(r, s): "WigiStackedBar range: " + r;
		WigiDatepicker(d, __): "Wigidatepicker date: " + date2string(getValue(d));
		WigiCalculator():"Calculator";
		WigiHTML(url, w, h, s):"Html resource: " + url;
		WigiReferenceDocument(t, d):"Reference document";
		WigiSetClipboardButton(title, text): "Set to clipboard button: " + title;
		WigiChoiceBlock(formula, choices): "Choice formula: " + formula;
		WigiExternalBlock(exType, content): "External block: " + exType;
		WigiFeedback(condition, description): "Feedback: " + condition;
		WigiFeedbackBlock(formula, visibilityCondition, interactiveType, choices): "Feedback formula: " + formula;
		WigiFillBlankBlock(width, caseSensitive, exactMatch, mathFeatures, answers, otherAnswer): "Fill-blank template";
	}
}
