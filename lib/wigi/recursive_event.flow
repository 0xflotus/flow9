import wigi/events;
import wigi/comap_control;
import wigi/table_event;

export {
	// Get the chance to handle the given event before it is sent along to the children. Return true if handled here
	wigiRecursiveEvent(state : WigiEditorState, el : WigiRecursiveType, event : WigiEvent) -> bool;
}

wigiRecursiveEvent(state : WigiEditorState, el : WigiRecursiveType, event : WigiEvent) -> bool {
	switch (el : WigiRecursiveType) {
		WigiBrackets(leftBracketType, rightBracketType): false;
		WigiComap(collapsed): {
			handleComapEvent(state, el, event);
		}
		WigiDottedElement(left, right, top, bottom, leftSign, rightSign): false;
		WigiFraction(): false;
		WigiProduct(): false;
		WigiEvaluatingIntegral(): false;
		WigiRoot(): false;
		WigiLongDivision(): false;
		WigiIntegral(): false;
		WigiIntegral2(signType): false;
		WigiTopModificator(type, size): false;
		WigiSum(): false;
		WigiSupscript(): false;
		WigiSubscript(): false;
		WigiSupSubscript(): false;
		WigiOverscript(): false;
		WigiUnderscript(): false;
		WigiOverlay(): false;
		WigiAltText(): false;
		WigiTable(r, c, cs, s): false;
		WigiInteractiveTable(f, r, s): false;
		WigiTableCell(wh, cs): handleTableEvent(state, el, event);
		WigiSpreadsheetCell(wh, em, cs): handleTableEvent(state, el, event);
		WigiFrame(w, h, s): false;
		WigiCode(sc, sr): false;
		WigiTyped(typ): false;
		WigiButton(w, s): false;
		WigiHyperlink(u): false;
		WigiRadioOption(i, v): false;
		WigiVisible(__): false;
		WigiVersions(__): false;
		WigiExternalRecursive(exType, content): false;
	}
}
